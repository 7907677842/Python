# Can you figure out how to find the sum and product of numbers from 1 to N?
def sumOrProduct(n, q):

    # Write your Code here.
    sum=0
    if(q==1):
        for i in range(1,n+1):

            sum=sum+i
        print(sum)

    else:
        product=1
        for i in range(1,n+2):

            product=product*i
        print(product)
sumOrProduct(4,2)
